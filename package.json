{
  "name": "web_performance_optimization",
  "version": "1.0.0",
  "description": "Web Performance Optimization Workshop ",
  "main": "server.js",
  "scripts": {
    "watch": "parcel watch index.html",
    "build": "parcel build index.html && npm run critical",
    "start": "node server.js",
    "prod": "npm-run-all build start",
    "dev": "npm-run-all --parallel watch start",
    "generate:eot": "ls fonts/**/*.ttf | sed -e 's/\\.ttf//g' | xargs -I % ttf2eot %.ttf %.eot",
    "generate:woff": "ls fonts/**/*.ttf | sed -e 's/\\.ttf//g' | xargs -I % ttf2woff %.ttf %.woff",
    "generate:woff2": "ls fonts/**/*.ttf | sed -e 's/\\.ttf//g' | xargs -I % sh -c \"cat %.ttf | ttf2woff2 > %.woff2\"",
    "fonts:generate": "npm-run-all --parallel generate:*",
    "fonts:subset": "node scripts/fontSubset.js",
    "optimise:jpeg": "node scripts/optimiseJpeg.js",
    "generate:webp": "node scripts/jpegToWebP.js",
    "generate:sprites": "svg-sprite --css --css-render-css --css-dest=css --css-sprite=../images/icons.svg --css-layout=diagonal images/icons/*.svg",
    "print:selectors": "longest-selectors css/styles.css",
    "critical": "node scripts/critical.js"
  },
  "author": "Mateusz Kwasniewski <kwasniewski.mateusz@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "blazy": "1.8.2",
    "compression": "1.7.4",
    "critical": "2.0.0-23",
    "express": "4.16.3",
    "iltorb": "2.4.4",
    "jquery": "3.4.1",
    "mime": "2.4.4",
    "node-zopfli-es": "1.0.3",
    "quicklink": "2.0.0-alpha",
    "shrink-ray-current": "4.1.2"
  },
  "devDependencies": {
    "fontmin": "0.9.8",
    "imagemin": "7.0.1",
    "imagemin-mozjpeg": "8.0.0",
    "imagemin-webp": "5.1.0",
    "longest-selectors": "1.1.0",
    "npm-run-all": "4.1.5",
    "parcel-bundler": "1.12.4",
    "parcel-plugin-lazy": "0.0.4",
    "parcel-plugin-purgecss": "2.1.3",
    "svg-sprite": "1.5.0",
    "ttf2eot": "2.0.0",
    "ttf2woff": "2.0.1",
    "ttf2woff2": "3.0.0"
  }
}
